<% if (title) { %>
    <h1><%= title %></h1>
    <% } else {
        res.redirect('/')
    } %>
    <div class="flash-message"><%- messages() %>
        <% if (errors) { %>
            <ul class="notice">
           <% errors.array().forEach(error => { %>
             <li><%= error.msg %></li>
          <%  }) %>
           </ul>
          <% } %>
    </div>

    <table id="messagesTable">
        
        <tbody>
          <% showMessages.forEach(msg => { %>
            <tr>
                <td><%= msg.message_message %></td>
                <td><%= msg.message_email %></td>
                <td><%= msg.inv_year %> <%= msg.inv_make %> <%=msg.inv_model %></td>
              
            </tr>
          <% }) %>
        </tbody></table>